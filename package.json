{
    "name": "frontend",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\" \"npm run start-electron\"",
        "start-backend": "uvicorn pipeline.app.main:app --reload --host localhost --port 8000",
        "start-frontend": "cd website && vite",
        "start-electron": "electron electron/main.cjs"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "electron": "^36.3.1",
        "vite": "^5.2.0"
    }
}
